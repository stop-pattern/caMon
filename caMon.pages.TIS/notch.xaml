<Page x:Class="caMon.pages.TIS.notch"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:caMon.pages.TIS"
      mc:Ignorable="d" 
      Title="notch">
    <Page.Resources>
        <Style TargetType="Border" >
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="2.5"/>
            <Setter Property="Padding" Value="0"/>
        </Style>
    </Page.Resources>

    <Viewbox>
        <Grid x:Name="cells" Width="120" Height="750">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Border Grid.Row="0">
                <local:CustomIndicator x:Name="ind_e" status="False" text="EB" color_back="Red" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="1">
                <local:CustomIndicator x:Name="ind_b8" Grid.Row="1" status="False" text="B8" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="2">
                <local:CustomIndicator x:Name="ind_b7" Grid.Row="2" status="False" text="B7" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="3">
                <local:CustomIndicator x:Name="ind_b6" Grid.Row="3" status="False" text="B6" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="4">
                <local:CustomIndicator x:Name="ind_b5" Grid.Row="4" status="False" text="B5" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="5">
                <local:CustomIndicator x:Name="ind_b4" Grid.Row="5" status="False" text="B4" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="6">
            <local:CustomIndicator x:Name="ind_b3" Grid.Row="6" status="False" text="B3" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="7">
            <local:CustomIndicator x:Name="ind_b2" Grid.Row="7" status="False" text="B2" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="8">
            <local:CustomIndicator x:Name="ind_b1" Grid.Row="8" status="False" text="B1" color_back="Orange" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="9">
            <local:CustomIndicator x:Name="ind_n" Grid.Row="9" status="False" text="N" color_back="LimeGreen" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="10">
            <local:CustomIndicator x:Name="ind_p1" Grid.Row="10" status="False" text="P1" color_back="SkyBlue" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="11">
            <local:CustomIndicator x:Name="ind_p2" Grid.Row="11" status="False" text="P2" color_back="SkyBlue" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="12">
            <local:CustomIndicator x:Name="ind_p3" Grid.Row="12" status="False" text="P3" color_back="SkyBlue" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="13">
            <local:CustomIndicator x:Name="ind_p4" Grid.Row="13" status="False" text="P4" color_back="SkyBlue" color_text="Black" color_disable="White"/>
            </Border>
            <Border Grid.Row="14">
            <local:CustomIndicator x:Name="ind_p5" Grid.Row="14" status="False" text="P5" color_back="SkyBlue" color_text="Black" color_disable="White"/>
            </Border>
        </Grid>
    </Viewbox>
</Page>
